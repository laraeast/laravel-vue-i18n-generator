{
    "name": "laraeast/laravel-vue-i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": ["laravel","vue-i18n"],
    "homepage": "http://github.com/laraeast/laravel-vue-i18n-generator",
    "authors": [
        {
            "name": "Martin Lindhe",
            "email": "martin@ubique.se"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": "~5.1|~6.0|~7.0|~8.0",
        "illuminate/support": "~5.1|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "Laraeast\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraeast\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
